            "image": "nu_bet"
        },
        {
            "type": "single",
            "name": "Non-uniformity corrected",
            "image": "nu_bet",
            "nslices": 3
        },
        {
            "type": "single",
            "name": "Non-uniformity corrected",
            "image": "nu_bet",
            "qcform": false
        },
        {
            "type": "contour",
            "name": "Non-uniformity corrected",
            "image": "nu_bet",
            "labelimage": "transformed_atlas"
        },
        {
            "type": "contour",
            "name": "Non-uniformity corrected",
            "image": "nu_bet",
            "labelimage": "transformed_atlas",
